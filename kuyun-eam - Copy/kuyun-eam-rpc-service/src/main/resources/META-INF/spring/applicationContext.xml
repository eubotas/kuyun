<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- ApplicationContext -->
    <bean id="springContextUtil" class="com.kuyun.common.util.SpringContextUtil"></bean>
    <bean id="jpushUtil" class="com.kuyun.common.jpush.JpushUtil"></bean>
    <bean id="areaUtil" class="com.kuyun.eam.util.AreaUtil"></bean>
    <bean id="springMailConfig" class="com.kuyun.common.mail.SpringMailConfig"></bean>
    <bean id="emailService" class="com.kuyun.common.mail.service.EmailService"></bean>
    <bean id="alarmTypeFactory" class="com.kuyun.eam.alarm.AlarmTypeFactory"></bean>
    <bean id="valAboveHandler" class="com.kuyun.eam.alarm.ValAboveHandler"></bean>
    <bean id="valBelowHandler" class="com.kuyun.eam.alarm.ValBelowHandler"></bean>
