<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://code.alibabatech.com/schema/dubbo
        http://code.alibabatech.com/schema/dubbo/dubbo.xsd ">

    <dubbo:registry address="zookeeper://${zkserver}:2181" default="false"/>
    <dubbo:registry id="eamRegistry" address="zookeeper://${zkserver}:2181"/>



    <!-- 订阅服务 -->
    <dubbo:consumer check="false"/>


    <dubbo:reference id="eamTicketTypeService" interface="com.kuyun.eam.rpc.api.EamTicketTypeService" mock="true" registry="eamRegistry"/>
</beans>